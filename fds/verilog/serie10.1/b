#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-513-gef7f0a8f3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib64/ivl/system.vpi";
:vpi_module "/usr/local/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib64/ivl/va_math.vpi";
S_0x9086c0 .scope module, "timer_db" "timer_db" 2 1;
 .timescale 0 0;
v0x91c860_0 .var "clk", 0 0;
v0x91c920_0 .net "f", 0 0, v0x91c2a0_0;  1 drivers
v0x91c9f0_0 .var "in", 0 0;
v0x91caf0_0 .var "reset", 0 0;
S_0x908850 .scope module, "my_timer" "timer" 2 8, 3 1 0, S_0x9086c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "in";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /OUTPUT 1 "f";
P_0x908a30 .param/l "STATE_A" 0 3 7, C4<01>;
P_0x908a70 .param/l "STATE_B" 0 3 8, C4<10>;
P_0x908ab0 .param/l "STATE_IDLE" 0 3 6, C4<00>;
v0x908e20_0 .net "clk", 0 0, v0x91c860_0;  1 drivers
v0x91c2a0_0 .var "f", 0 0;
v0x91c360_0 .net "in", 0 0, v0x91c9f0_0;  1 drivers
v0x91c430_0 .var "next_state", 1 0;
v0x91c510_0 .net "reset", 0 0, v0x91caf0_0;  1 drivers
v0x91c620_0 .var "state", 1 0;
v0x91c700_0 .var "timer", 3 0;
E_0x8cca60 .event anyedge, v0x91c620_0;
E_0x904d10 .event posedge, v0x908e20_0;
E_0x9048b0 .event anyedge, v0x91c510_0, v0x91c360_0, v0x91c620_0, v0x91c700_0;
    .scope S_0x908850;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x91c700_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x91c620_0, 0, 2;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x91c430_0, 0, 2;
    %end;
    .thread T_0;
    .scope S_0x908850;
T_1 ;
    %wait E_0x9048b0;
    %load/vec4 v0x91c510_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/1 T_1.2, 8;
    %load/vec4 v0x91c360_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_or 8, 9;
T_1.2;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x91c430_0, 0, 2;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x91c700_0, 0, 4;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x91c620_0;
    %pushi/vec4 1, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x91c700_0;
    %pad/u 32;
    %pushi/vec4 9, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.3, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x91c430_0, 0, 2;
    %jmp T_1.4;
T_1.3 ;
    %load/vec4 v0x91c360_0;
    %load/vec4 v0x91c620_0;
    %pushi/vec4 0, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x91c430_0, 0, 2;
T_1.5 ;
T_1.4 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x908850;
T_2 ;
    %wait E_0x904d10;
    %load/vec4 v0x91c430_0;
    %store/vec4 v0x91c620_0, 0, 2;
    %load/vec4 v0x91c620_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_2.0, 4;
    %load/vec4 v0x91c700_0;
    %addi 1, 0, 4;
    %store/vec4 v0x91c700_0, 0, 4;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x908850;
T_3 ;
    %wait E_0x8cca60;
    %load/vec4 v0x91c620_0;
    %pushi/vec4 2, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %store/vec4 v0x91c2a0_0, 0, 1;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x9086c0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91c860_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91caf0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91c9f0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x9086c0;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x91c860_0;
    %inv;
    %store/vec4 v0x91c860_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x9086c0;
T_6 ;
    %vpi_call 2 19 "$monitor", "f = ", v0x91c920_0, " next state = ", v0x91c430_0, " timer = ", v0x91c700_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91caf0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
