v0x12f3970_0 .net "in", 2 0, v0x12f3bb0_0;  1 drivers
v0x12f3a70_0 .var "out", 7 0;
E_0x12e0420 .event anyedge, v0x12f3970_0;
    .scope S_0x12f36e0;
T_0 ;
    %wait E_0x12e0420;
    %load/vec4 v0x12f3970_0;
