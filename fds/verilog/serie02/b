t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x12dfca0_0, 0, 32;
T_1.0 ; Top of for-loop
    %load/vec4 v0x12dfca0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x12dfca0_0;
