    %addi 1, 0, 32;
    %store/vec4 v0x12dfca0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ; for-loop exit label
    %end;
    .scope S_0x12df930;
t_0 %join;
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_1;
