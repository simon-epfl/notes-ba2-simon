    %ix/vec4 4;
    %shiftl 4;
    %cmp/ne;
    %jmp/0xz  T_1.3, 4;
    %vpi_call 2 27 "$display", "[%d] Error! Wrong Output!", v0x12dfca0_0 {0 0 0};
    %jmp T_1.4;
T_1.3 ;
    %vpi_call 2 29 "$display", "[%d] Correct Output!", v0x12dfca0_0 {0 0 0};
T_1.4 ;
T_1.2 ; for-loop step statement
